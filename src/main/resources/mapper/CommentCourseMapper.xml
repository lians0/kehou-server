<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.kehou.mapper.CommentCourseMapper">

    <resultMap id="BaseResultMap" type="com.example.kehou.domain.entity.CommentCourse">
        <id property="commentId" column="comment_id" jdbcType="INTEGER"/>
        <result property="userId" column="user_id" jdbcType="INTEGER"/>
        <result property="subjectId" column="subject_id" jdbcType="INTEGER"/>
        <result property="semesterId" column="semester_id" jdbcType="INTEGER"/>
        <result property="testType" column="test_type" jdbcType="VARCHAR"/>
        <result property="commentText" column="comment_text" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Select">
        select comment_id,
               user_id,
               subject_id,
               semester_id,
               test_type,
               comment_text
        from comment_course
    </sql>
</mapper>
